<?xml version="1.0" encoding="GBK"?>
<!-- Spring配置文件的根元素，使用spring-beans-3.1.xsd语义约束 -->
<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	">
	<bean id="loginAction" class="com.tbn.login.web.action.LoginAction">
	   <property name="loginService" ref="loginService"></property>
	</bean>
    <bean id="UserDao" class="com.tbn.login.dao.UserDaoImpl" parent="daoParent"></bean>
    <bean id="loginService" class="com.tbn.login.service.LoginService">
       <property name="userDao" ref="UserDao"></property>
    </bean>
</beans>

